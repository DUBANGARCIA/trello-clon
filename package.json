{
  "name": "trello-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "pnpm ng",
    "commit": "pnpm git-cz",
    "start": "pnpm ng serve",
    "build": "pnpm ng build",
    "watch": "pnpm ng build --watch --configuration development",
    "test": "pnpm ng test",
    "prepare": "pnpm husky",
    "postinstall": "pnpm sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@biomejs/biome": "^1.8.3",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jasmine": "~5.1.0",
    "commitizen": "^4.3.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.4",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
